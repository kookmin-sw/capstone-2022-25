<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.holyseat"
    tools:ignore="LockedOrientationActivity">

    <application
        android:name=".scene.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HolySeat">
        <activity
            android:name=".scene.joinmembership.JoinMemberShip"
            android:exported="false" />
        <activity
            android:name=".scene.idpwfind.IdPwFindActivity"
            android:exported="false" />
        <activity
            android:name=".scene.recentreview.TwelveActivity"
            android:exported="false" />
        <activity
            android:name=".scene.splash.SplashActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".scene.main.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
    </application>

</manifest>